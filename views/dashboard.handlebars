<hr>

<!-- MY PROFILE -->
<div class="containter-fluid">
    <div class="row">
        <div class="col-sm-4">
            <h3 class="text-black-20 mx-auto mt-2 mb-3">User Profile</h3>
            <h5 class="text-black-20 mx-auto mt-2 mb-0">Welcome Liberty</h5>
            <h5 class="text-black-20 mx-auto mt-2 mb-5">Beans Member</h5>

            <a href="/logout">
                <i class="glyphicon glyphicon-user"></i>
                Log Out </a>
        </div>

        <div class="col-sm-4">
            <div class="card">
                <div class="card-header">
                    <h3 class="text-black-20 mx-auto mt-2 mb-0">Likes</h3>

                </div>
                <div class="card-body">
                    <p>Double Donut Cofffee
                    </p>
                </div>
            </div>
        </div>
        <div class="col-sm-4">
            <div class="card">
                <div class="card-header">
                    <h3 class="text-black-20 mx-auto mt-2 mb-0">Disikes</h3>

                </div>
                <div class="card-body">
                    <p>Random Coffee</p>
                </div>
            </div>
        </div>
    </div>
</div>
<hr>
<br>

<div class="container-fluid">
    <div class="row">
        {{#each coffees}}
        <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
            <div class="card h-250">
                <img style="height: 200px;" class="card-img-top" src="{{this.img}}" alt="Coffee Image">
                <div class="card-body">
                    <h5 class="card-title">{{this.name}}</h5>
                    <p class="card-text">{{this.company}}</p>
                    <p class="card-text">Type: {{this.flavor}}</p>
                    <p class="card-text">Caffinated: {{this.caf}}</p>

                    <center>
                        <button type="button" class="btn btn-success like" data-id="{{id}}" value="true"
                            data-newrating="{{like}}">Like</button>

                        <button type="button" class="btn btn-danger dislike" data-id="{{id}}" value="false"
                            data-newrating="{{like}}">Dislike</button>
                    </center>
                </div>
            </div>
        </div>
        {{/each}}

    </div>
</div>
<hr>

