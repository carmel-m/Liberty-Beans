<!-- MY PROFILE -->
<div class="container">
    <div class="row profile">
        <div class="col-md-3">
            <div class="profile-sidebar">
                <!-- SIDEBAR USERPIC -->
                <div class="profile-userpic">
                    <img src="http://keenthemes.com/preview/metronic/theme/assets/admin/pages/media/profile/profile_user.jpg"
                        class="img-responsive" alt="">
                </div>
                <!-- END SIDEBAR USERPIC -->
                <!-- SIDEBAR USER TITLE -->
                <div class="profile-usertitle">
                    <div class="profile-usertitle-name">
                        <h3>Joseph Schmoseff</h3>
                    </div>
                    <div class="profile-usertitle-job">
                        Welcome Liberty Beans Member
                    </div>
                </div>
                <!-- END SIDEBAR USER TITLE -->

                <!-- SIDEBAR MENU -->
                <div class="profile-usermenu">
                    <ul class="nav">
                        <li class="active">
                            <a href="#">
                                <i class="glyphicon glyphicon-home"></i>
                                Your Liked Coffees </a>
                        </li>
                        {{#each relations.relation}}
                        <div class="likedlist">

                            <h4>{{name}}</h4>
                        </div>
                        {{/each}}
                        <li>
                            <a href="https://www.amazon.com/s?k=coffee&ref=nb_sb_noss_1" target="_blank">
                                <i class="glyphicon glyphicon-ok"></i>
                                Amazon Suggestions </a>
                        </li>
                    </ul>
                </div>

                <hr>

                <ul>
                    <a href="https://young-forest-75020.herokuapp.com/">
                        <i class="glyphicon glyphicon-user"></i>
                        Log Out </a>
                </ul>
                <!-- END MENU -->
            </div>
        </div>

    </div>
</div>

<div class="container">
    <div class="row">
        {{#each coffees}}
        <div class="col-sm-3">
            <div class="card h-250">
                <img style="height: 200px;" class="card-img-top" src="{{this.img}}" alt="Coffee Image">
                <div class="card-body">
                    <h5 class="card-title">{{this.name}}</h5>
                    <p class="card-text">{{this.company}}</p>
                    <p class="card-text">Type: {{this.flavor}}</p>
                    <p class="card-text">Caffinated: {{this.caf}}</p>
                    <div class="btn-group btn-group-toggle" data-toggle="buttons">
                        <label class="btn btn-success like2">
                            <input type="radio" name="options" data-id="{{id}}" value="true" data-newrating="{{like}}"
                                autocomplete="off" checked>
                            Like
                        </label>
                        <label class="btn btn-danger like2">
                            <input type="radio" name="options" data-id="{{id}}" value="false" data-newrating="{{like}}"
                                autocomplete="off"> Disliked
                        </label>
                    </div>

                </div>
            </div>
        </div>
        {{/each}}

    </div>
</div>


<!-- Footer -->
<footer class="bg-black small text-center text-white-50">
    <div class="container">
        <h4>
            Copyright &copy; Liberty Beans - Mustafa Muhammad - Carmel Meyer - Moshe Stern - Keaton Sentak 2019
        </h4>
    </div>
</footer>

<!-- Bootstrap core JavaScript -->
<script src="vendor/jquery/jquery.min.js"></script>
<script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

<!-- Plugin JavaScript -->
<script src="vendor/jquery-easing/jquery.easing.min.js"></script>
<!-- Custom scripts for this template -->
<script src="js/grayscale.min.js"></script>
<script src="js/index.js"></script>

